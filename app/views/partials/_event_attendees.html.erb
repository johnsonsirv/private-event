<div id="attendees">
	<div class="card">
			<div class="card-body">
					<h5 class="card-title"> Also attending this event </h5>
					<% @event_details.attendees.each do |attendee| %>
						<%= content_tag :span, class: "badge badge-info" do -%>
							<%= attendee.fullname.titlecase %>
						<% end -%>
					<% end %>
			</div>
		</div>
</div>